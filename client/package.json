{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "apollo-upload-client": "^17.0.0",
    "formik": "^2.2.9",
    "graphql": "^15.8.0",
    "history": "^5.3.0",
    "howler": "^2.2.3",
    "i18next": "^22.4.9",
    "i18next-http-backend": "^2.1.1",
    "jwt-decode": "^3.1.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^12.1.5",
    "react-lazy-load": "^4.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-spring": "^9.5.5",
    "styled-components": "^5.3.9",
    "styled-react-modal": "^3.0.1",
    "typescript": "^4.7.4",
    "wavesurfer.js": "^6.2.0",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "generate": "graphql-codegen --config codegen.yml",
    "schema:get": "apollo client:download-schema --endpoint=http://localhost:8080/graphql ./src/graphql/schema.graphql",
    "schema:remove": "rm ./src/graphql/schema.graphql",
    "generate:clear": "find . -name _gen_ -type d -exec rm -r {} +",
    "gen": "npm run schema:remove && npm run generate:clear && npm run schema:get && npm run generate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/fragment-matcher": "^3.3.0",
    "@graphql-codegen/near-operation-file-preset": "^2.4.1",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-compatibility": "^2.1.5",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.2",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/howler": "^2.2.7",
    "@types/styled-components": "^5.1.25",
    "@types/styled-react-modal": "^1.2.2",
    "@types/wavesurfer.js": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "prettier": "^2.7.1",
    "react-error-overlay": "^6.0.11"
  }
}
